

@model IEnumerable<WEBSoLienLacDienTu.Areas.Admin.Code.QRCodeModel>
@{
    ViewBag.Title = "IndexQR";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2 class="title-page">Mã QR Tự Phát Sinh Theo Lớp</h2>


<div class="table-responsive">
    <table class="table table-bordered" id="dataTable" cellspacing="0">
        <thead>
            <tr>
                <th>ID</th>
                <th>QR</th>               
            </tr>
        </thead>

        <tbody id="myTable">
            @foreach (var item in Model)
            {
                <tr>
                    
                    <td>
                        @Html.DisplayFor(modelitem => item.QRCodeText)
                    </td>
                   
                    <td>
                        <img src="@Url.Content(item.QRCodeImagePath)" alt="Image" />
                    </td>
                    
                </tr>
            }
        </tbody>
    </table>
</div>

<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>



